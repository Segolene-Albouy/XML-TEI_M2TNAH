            <ref name="valItem"/>
         </zeroOrMore>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.deprecated.attribute.validUntil"/>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="mode"
                       a:defaultValue="add">
               <a:documentation>précise l'effet de cette déclaration sur son parent.</a:documentation>
               <choice>
                  <value>add</value>
                  <a:documentation>cette déclaration s'ajoute aux définitions courantes.</a:documentation>
                  <value>delete</value>
                  <a:documentation/>
                  <value>change</value>
                  <a:documentation/>
                  <value>replace</value>
                  <a:documentation/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="type"
                       a:defaultValue="open">
               <a:documentation>précise l'extensibilité de la liste des valeurs de l'attribut.</a:documentation>
               <choice>
                  <value>closed</value>
                  <a:documentation>seules les valeurs indiquées sont autorisées.</a:documentation>
                  <value>semi</value>
                  <a:documentation>(semi-ouvert ) toutes les valeurs indiquées doivent être acceptées, mais d'autres valeurs sont acceptables et le logiciel doit avoir une procédure qui leur est adaptée.</a:documentation>
                  <value>open</value>
                  <a:documentation>les valeurs indiquées ne sont que des valeurs d'exemple.</a:documentation>
