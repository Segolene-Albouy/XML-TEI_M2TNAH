   <define name="relation">
      <element name="relation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(relation) décrit tout type de relation ou de lien à l'intérieur d'un groupe donné de participants. [13.3.2.3. Personal Relationships]</a:documentation>
         <optional>
            <ref name="desc"/>
         </optional>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-relation-relationRegex-constraint-rule-16">
            <s:rule xmlns:xi="http://www.w3.org/2001/XInclude"
                    xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:TEI//tei:relation">
               <s:assert test="matches(@mutual, '#[a-z]{4}\s#[a-z]{4}') or matches(@active, '#[a-z]{4}') and matches(@passive, '#[a-z]{4}')">
                              Une &lt;relation&gt; mutuelle doit être décrite avec deux références à des @xml:id de personnes,
                              composées chacune d'un '#' et de 4 lettres et séparées d'un espace. Une relation active/passive doit être décrite avec
                              deux attributs @passive et @active, chacun contenant une référence à un xml:id composé d'un '#' et de 4 lettres.</s:assert>
            </s:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-relation-reforkeyorname-constraint-assert-15">
            <rule context="tei:relation">
               <assert xmlns:xi="http://www.w3.org/2001/XInclude"
                       xmlns:rng="http://relaxng.org/ns/structure/1.0"
                       xmlns:s="http://purl.oclc.org/dsdl/schematron"
                       test="@ref or @key or @name">One of the attributes  'name', 'ref' or 'key' must be supplied</assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-relation-activemutual-constraint-report-11">
            <rule context="tei:relation">
               <report xmlns:xi="http://www.w3.org/2001/XInclude"
                       xmlns:rng="http://relaxng.org/ns/structure/1.0"
