         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.datable.w3c.attribute.when"/>
         <ref name="att.datable.w3c.attribute.notBefore"/>
         <ref name="att.datable.w3c.attribute.notAfter"/>
         <ref name="att.datable.w3c.attribute.from"/>
         <ref name="att.datable.w3c.attribute.to"/>
         <ref name="att.datable.custom.attribute.when-custom"/>
         <ref name="att.datable.custom.attribute.notBefore-custom"/>
         <ref name="att.datable.custom.attribute.notAfter-custom"/>
         <ref name="att.datable.custom.attribute.from-custom"/>
         <ref name="att.datable.custom.attribute.to-custom"/>
         <ref name="att.datable.custom.attribute.datingPoint"/>
         <ref name="att.datable.custom.attribute.datingMethod"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">caractérise le titre selon une typologie adaptée.</a:documentation>
               <choice>
                  <value>main</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">titre principal</a:documentation>
                  <value>sub</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(titre de
niveau inférieur, titre de partie) sous-titre, titre de partie.</a:documentation>
                  <value>alt</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(titre
alternatif, souvent dans une autre
langue, par lequel l'oeuvre est
également connu) autre titre, souvent exprimé dans une autre langue, par lequel l'ouvrage est aussi connu</a:documentation>
                  <value>short</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">forme abrégée du titre</a:documentation>
                  <value>desc</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(paraphrase descriptive de l'oeuvre
ayant les fonctions d'un titre) paraphrase descriptive de l'oeuvre fonctionnant comme un titre</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="publisher">
      <element name="publisher">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(éditeur) donne le nom de l'organisme responsable de la publication ou de la distribution d'un élément de la bibliographie. [3.12.2.4. Imprint, Size of a Document, and Reprint Information 2.2.4. Publication, Distribution, Licensing, etc.]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <empty/>
      </element>
   </define>
   <define name="pubPlace">
      <element name="pubPlace">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lieu de publication) contient le nom du lieu d'une publication. [3.12.2.4. Imprint, Size of a Document, and Reprint Information]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <empty/>
      </element>
   </define>
   <define name="bibl">
      <element name="bibl">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(référence bibliographique.) contient une référence bibliographique faiblement structurée dans laquelle les sous-composants peuvent ou non être explicitement balisés. [3.12.1. Methods of Encoding Bibliographic References and Lists of References 2.2.7. The Source Description 15.3.2. Declarable Elements]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.gLike"/>
               <ref name="model.highlighted"/>
               <ref name="model.pPart.data"/>
               <ref name="model.pPart.edit"/>
               <ref name="model.segLike"/>
               <ref name="model.ptrLike"/>
               <ref name="model.biblPart"/>
               <ref name="model.global"/>
            </choice>
         </zeroOrMore>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.sortable.attributes"/>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="default"
                       a:defaultValue="false">
               <a:documentation>Indique si oui ou non cet élément est affecté par défaut quand son élément parent a été sélectionné.</a:documentation>
               <choice>
                  <value>true</value>
                  <a:documentation/>
                  <value>false</value>
                  <a:documentation>cet élément ne peut être sélectionné qu'explicitement, à moins qu'il ne soit le seul de ce type, auquel cas il est sélectionné si son parent a été choisi</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="status"
                       a:defaultValue="draft">
               <a:documentation>describes the status of a document either currently or, when associated with a dated element, at the time indicated.</a:documentation>
               <choice>
                  <value>approved</value>
                  <a:documentation/>
                  <value>candidate</value>
                  <a:documentation/>
                  <value>cleared</value>
                  <a:documentation/>
                  <value>deprecated</value>
                  <a:documentation/>
                  <value>draft</value>
                  <a:documentation/>
                  <value>embargoed</value>
                  <a:documentation/>
                  <value>expired</value>
                  <a:documentation/>
                  <value>frozen</value>
                  <a:documentation/>
                  <value>galley</value>
                  <a:documentation/>
                  <value>proposed</value>
                  <a:documentation/>
                  <value>published</value>
                  <a:documentation/>
                  <value>recommendation</value>
                  <a:documentation/>
                  <value>submitted</value>
                  <a:documentation/>
                  <value>unfinished</value>
                  <a:documentation/>
                  <value>withdrawn</value>
                  <a:documentation/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="textLang">
      <element name="textLang">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(langues du texte) décrit les langues et systèmes d'écriture utilisés dans un manuscrit (et non dans la description du manuscrit, dont les langues et systèmes d'écriture sont décrits dans l'élément <code xmlns="http://www.w3.org/1999/xhtml">&lt;langUsage&gt;</code>). [3.12.2.4. Imprint, Size of a Document, and Reprint Information 10.6.6. Languages and Writing Systems]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <empty/>
      </element>
   </define>
   <define name="app">
      <element name="app">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(entrée d'apparat critique) contient une entrée dans un apparat critique, constituée d'un lemme facultatif et d'au moins une leçon. [12.1.1. The Apparatus Entry]</a:documentation>
         <group>
            <optional>
               <ref name="lem"/>
            </optional>
            <zeroOrMore>
               <choice>
                  <ref name="model.rdgLike"/>
                  <ref name="model.noteLike"/>
                  <ref name="witDetail"/>
                  <ref name="rdgGrp"/>
               </choice>
            </zeroOrMore>
         </group>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <optional>
            <attribute name="corresp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(correspond) <code xmlns="http://www.w3.org/1999/xhtml">@corresp</code> sert à indiquer le type de variation qu'il y a entre deux témoins ou plus.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="lem">
      <element name="lem">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lemme) contient le lemme ou le texte de base d'une variante textuelle [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.divLike"/>
               <ref name="model.divPart"/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.inter"/>
               <ref name="model.global"/>
               <ref name="model.rdgPart"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-lem-lemWit-constraint-rule-9">
            <s:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:TEI//tei:lem">
               <s:assert test="./@wit and ./@wit[contains(., '#brl')]">
                              L'élément &lt;lem&gt; doit avoir un attribut @wit
                              qui contient '#brl'</s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.written.attribute.hand"/>
         <optional>
            <attribute name="wit">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(témoin ou témoins) Indique le témoin grâce à son identifiant <code xmlns="http://www.w3.org/1999/xhtml">@xml:id</code>
               </a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>#atw</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Antwerp M. 15.6</a:documentation>
                        <value>#brl</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Berlin Rehdiger 3, la leçon principale de cette édition critique</a:documentation>
                        <value>#brn</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Bern A14</a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="rdg">
      <element name="rdg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(leçon) contient une leçon à l'intérieur d'une variante du texte. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.divLike"/>
               <ref name="model.divPart"/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.inter"/>
               <ref name="model.global"/>
               <ref name="model.rdgPart"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-rdg-rdgWit-constraint-rule-10">
            <s:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:TEI//tei:rdg">
               <s:assert test="./@wit and ./@wit[not(contains(., '#brl'))]">
                              L'élément &lt;rdg&gt; doit contenir un @wit. 
                              La valeur de cet attribut ne peut pas contenir '#brl'</s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.written.attribute.hand"/>
         <optional>
            <attribute name="wit">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(témoin ou témoins) Indique le témoin grâce à son identifiant <code xmlns="http://www.w3.org/1999/xhtml">@xml:id</code>
               </a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>#atw</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Antwerp M. 15.6</a:documentation>
                        <value>#brl</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Berlin Rehdiger 3, la leçon principale de cette édition critique</a:documentation>
                        <value>#brn</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Bern A14</a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="lev">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Distance de Levenshtein (nombre entier) entre ce témoin et la leçon principale (témoin de Berlin)</a:documentation>
               <data type="token">
                  <param name="pattern">[^\p{C}\p{Z}]+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="rdgGrp">
      <element name="rdgGrp">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(groupe de leçons) L'élément <code xmlns="http://www.w3.org/1999/xhtml">&lt;rdgGrp&gt;</code> sert à réunir plusieurs témoins qui diffèrent d'autres groupes de témoins mais partagent une ou plusieurs caractéristiques communes. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <group>
            <optional>
               <ref name="lem"/>
            </optional>
            <zeroOrMore>
               <choice>
                  <ref name="model.rdgLike"/>
                  <ref name="model.noteLike"/>
                  <ref name="witDetail"/>
                  <ref name="rdgGrp"/>
               </choice>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-rdgGrp-only1lem-constraint-assert-8">
            <rule context="tei:rdgGrp">
               <sch:assert xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:s="http://purl.oclc.org/dsdl/schematron"
                           test="count(tei:lem) &lt; 2">Only one &lt;lem&gt; element may appear within a &lt;rdgGrp&gt;</sch:assert>
            </rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.written.attribute.hand"/>
         <optional>
            <attribute name="corresp">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(correspond) <code xmlns="http://www.w3.org/1999/xhtml">@corresp</code> sert à indiquer le type de variation qu'il y a entre deux témoins ou plus.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI"/>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="witDetail">
      <element name="witDetail">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(informations détaillées sur le témoin) L'élément <code xmlns="http://www.w3.org/1999/xhtml">&lt;witDetail&gt;</code> sert à donner des détails qui ne portent pas sur le contenu textuel des témoins. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.global"/>
               <ref name="bibl"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-witDetail-witDetailAttWit-constraint-rule-11">
            <s:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:TEI//tei:witDetail">
               <s:assert test="@wit and @wit/matches(., '(#[a-z]{3}\s?)+')">
                              Un élément &lt;witDetail&gt; doit contenir un attribut @wit.
                              La valeur du @wit doit être: une ou plusieurs séquences de "#" suivies de 3 lettres minuscules séparées d'un espace ('#brl #brn', '#atw'...)</s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.placement.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indication sur le type de détail décrit dans l'élément.</a:documentation>
               <choice>
                  <value>precision-about-page-numbering</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Valeur servant à indiquer un détail à propos de la numérotation des pages dans l'encodage.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <attribute name="wit">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(témoins) Indique le témoin grâce à son identifiant <code xmlns="http://www.w3.org/1999/xhtml">@xml:id</code>
            </a:documentation>
            <list>
               <oneOrMore>
                  <choice>
                     <value>#atw</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Antwerp M. 15.6</a:documentation>
                     <value>#brl</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Berlin Rehdiger 3, la leçon principale de cette édition critique</a:documentation>
                     <value>#brn</value>
                     <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renvoi au témoin Bern A14</a:documentation>
                  </choice>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="listWit">
      <element name="listWit">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(liste de témoins) donne une liste de définitions pour tous les témoignages cités dans un apparat critique, pouvant être groupées de façon hiérarchique. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <group>
            <optional>
               <ref name="model.headLike"/>
            </optional>
            <zeroOrMore>
               <ref name="desc"/>
            </zeroOrMore>
            <oneOrMore>
               <choice>
                  <ref name="witness"/>
                  <ref name="listWit"/>
               </choice>
            </oneOrMore>
         </group>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.sortable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="witness">
      <element name="witness">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(témoin) contient soit la description d'un seul témoin auquel il est fait référence à l'intérieur de l'apparat critique, soit une liste de témoins, à laquelle on doit faire référence par une seule abréviation [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.limitedPhrase"/>
               <ref name="model.inter"/>
               <ref name="note"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-witness-witnessID-constraint-rule-12">
            <s:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                    xmlns="http://www.tei-c.org/ns/1.0"
                    xmlns:s="http://purl.oclc.org/dsdl/schematron"
                    context="tei:TEI//tei:witness">
               <s:assert test="@xml:id and @xml:id/matches(., '[a-z]{3}')">
                              Les &lt;witness&gt; doivent avoir un identifiant unique @xml:id.
                              La valeur de cet attribut doit être composée de 3 lettres minuscules</s:assert>
            </s:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <ref name="att.sortable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="variantEncoding">
      <element name="variantEncoding">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Méthode d'encodage des variantes) sert à déclarer la méthode utilisée pour encoder les variantes critiques du texte. [12.1.1. The Apparatus Entry]</a:documentation>
         <empty/>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.attribute.n"/>
         <ref name="att.global.attribute.xmllang"/>
         <ref name="att.global.attribute.xmlbase"/>
         <ref name="att.global.attribute.xmlspace"/>
         <ref name="att.global.rendition.attribute.rend"/>
         <ref name="att.global.rendition.attribute.style"/>
         <ref name="att.global.rendition.attribute.rendition"/>
         <attribute name="method">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indique la méthode utilisée pour constituer l'apparat critique.</a:documentation>
            <choice>
               <value>location-referenced</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique se réfère aux numéros de ligne ou à tout autre schéma de référence canonique contenu dans le texte de base.</a:documentation>
               <value>double-end-point</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique indique la localisation précise du début et de la fin de chaque lemme dans le texte de base.</a:documentation>
               <value>parallel-segmentation</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">différentes leçons d’un passage sont données en parallèle dans le texte ; la notion de texte de base n’est pas nécessaire.</a:documentation>
            </choice>
         </attribute>
         <attribute name="location">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indique si les autres témoins sont contenus ou non dans ce fichier.</a:documentation>
            <choice>
               <value>internal</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique est intérieur au texte.</a:documentation>
               <value>external</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">l’apparat critique est extérieur au texte.</a:documentation>
            </choice>
         </attribute>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="oddbyexample-variantEncoding-location-variantEncodingLocation-constraint-rule-13">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:s="http://purl.oclc.org/dsdl/schematron"
                      context="tei:variantEncoding">
               <sch:assert test="(@location != 'external') or (@method != 'parallel-segmentation')">
              The @location value "external" is inconsistent with the
              parallel-segmentation method of apparatus markup.</sch:assert>
            </sch:rule>
         </pattern>
         <empty/>
      </element>
   </define>
