            <optional>
               <group>
                  <oneOrMore>
                     <ref name="model.descLike"/>
                  </oneOrMore>
                  <zeroOrMore>
                     <group>
                        <ref name="model.glossLike"/>
                        <zeroOrMore>
                           <ref name="model.descLike"/>
                        </zeroOrMore>
                     </group>
                  </zeroOrMore>
               </group>
            </optional>
         </group>
         <ref name="att.global.attribute.xmlid"/>
